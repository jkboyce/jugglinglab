<patternlist>
<title>Demo bounce patterns</title>

<line display="3 ball hyperlift (1.2 tps)" notation="siteswap"
pattern="pattern=3BHL;tps=1.2;hands=(15)(20,15)c(25)(20,-15)."/>
<line display="3 ball lift (1.6 tps)" notation="siteswap"
pattern="pattern=3BL;tps=1.6;hands=(20)(23,15)c(25)(23,-15)."/>
<line display="3 ball force (1.8 tps)" notation="siteswap"
pattern="pattern=3BF;tps=1.8;;hands=(25)(20)."/>
<line display="3 ball hyperforce (3.5 tps)" notation="siteswap"
pattern="pattern=3BHF;tps=3.5;hands=(25)(10)."/>

<line display=" "/>

<line display="7 ball hyperlift (3.5 tps)" notation="siteswap" pattern="pattern=7BHL;tps=3.5"/>
